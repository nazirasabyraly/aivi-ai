# PostgreSQL Database Configuration
POSTGRES_DB=nfac_db
POSTGRES_USER=nfac_user
POSTGRES_PASSWORD=your_strong_password_here

# Backend Configuration
SECRET_KEY=your_secret_key_here

DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Azure OpenAI Configuration (Optional - if using Azure instead of OpenAI)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-instance.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o
AZURE_OPENAI_API_VERSION=2024-02-01

# Riffusion Configuration (for music generation)
RIFFUSION_API_KEY=your_riffusion_api_key_here

# YouTube API (for video recommendations)
YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenAI Configuration (primary AI service)
OPENAI_API_KEY=sk-your_openai_api_key_here

# Email Service (Brevo/Sendinblue for email verification)
SMTP_API_KEY=your_brevo_api_key_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Server Configuration
HOST=0.0.0.0
PORT=8001
FRONTEND_URL=http://localhost:3000
ENVIRONMENT=development

# Email Configuration
FROM_EMAIL=noreply@yourdomain.com

# Session Configuration
SESSION_SECRET_KEY=your_session_secret_key_here
